<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8" />
    <link rel="icon" type="image/svg" href="/img/logo-0.png">
    <!--<link rel="apple-touch-icon" sizes="76x76" href="/img/apple-icon.png"> -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>Nextend</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

	<!-- Bootstrap core CSS     -->
	<link href="/css/bootstrap.min.css" rel="stylesheet" />
	<link href="/css/paper-kit.css?v=2.1.0" rel="stylesheet"/>

    <!--     Fonts and icons     -->
    <link href='//fonts.googleapis.com/css?family=Montserrat:400,300,700' rel='stylesheet' type='text/css'>
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <link href="/css/nucleo-icons.css" rel="stylesheet">

    <style>
        html,body {
            width: 100%;
            height: 100%;
        }
    </style>

</head>
<body>
    <div id="editor" style="width:800px;height:500px;margin-top:30px;margin-left:auto;margin-right:auto">
    <!-- <div id="editor" style="width:100%;height:100%;"></div> -->
</body>

<script src="/js/jquery-3.2.1.js" type="text/javascript"></script>
<script src="/js/popper.js" type="text/javascript"></script>
<script src="/js/bootstrap.min.js" type="text/javascript"></script>
<!-- <script src="/js/q5.min.js" type="text/javascript"></script> -->
<script src="/js/<%- process.env.NODE_ENV === 'production' ? 'q5.min.js' : 'q5.js' %>" type="text/javascript"></script>
<script type="text/javascript">
    $(function () {
        var server = q5.Server.create({
            baseUrl: '<%- process.env.API_SERVER %>',
            token: '<%- process.env.API_AUTHORIZATION_KEY %>',
        });

        server.loadWorkspace('myboundary', 'myfunction', new q5.Workspace())
            .then(workspace => {
                // workspace.setDirtyState(true);

                q5.createEditor(document.getElementById('editor'), workspace, server);
                //  {
                //     // navigationPanel: false, 
                //     // logsPanel: false,
                //     logsPanel: {
                //         maxSize: 5 * 1024
                //     },
                // })

            });
    });
</script>

</html>
