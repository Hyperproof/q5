{
  "name": "@5qtrs/pubsub",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "license": "UNLICENSED",
  "author": "FiveQuarters.io",
  "scripts": {
    "all": "yarn run -s lint && yarn run -s build && yarn run -s coverage",
    "build": "tsc -b",
    "test": "jest --colors",
    "coverage": "jest --colors --coverage",
    "lint": "tslint --fix -t stylish src/{,**/}*.ts{,x} test/{,**/}*.ts{,x}",
    "clean": "rm -r -f lib; rm -r -f coverage",
    "start": "node lib/index.js",
    "ngrok": "env LOGS_WS_PORT=${LOGS_WS_PORT:-5002} $(grep '^LOGS_WS_PORT=' .env) bash -c 'ngrok http $LOGS_WS_PORT'"
  },
  "keywords": [],
  "dependencies": {
    "dotenv": "^6.2.0",
    "jsonwebtoken": "^8.4.0",
    "ws": "^6.1.3",
    "zeromq": "^5.1.0",
    "@5qtrs/logger": "^0.0.1"
  },
  "devServer": {
    "xpubport": 5000,
    "xsubport": 5001
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/ws": "^6.0.1",
    "@types/zeromq": "^4.6.1"
  }
}
